            "type": "Microsoft.Storage/storageAccounts",
            "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
            },
            "kind": "Storage",
            "name": "[parameters('storageAccounts_caddiag161_name')]",
            "apiVersion": "2016-01-01",
            "location": "northeurope",
            "tags": {},
            "properties": {},
            "resources": [],
