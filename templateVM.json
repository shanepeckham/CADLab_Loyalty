        },
           "virtualMachines_CADSession2016LegacyAPI_name": {
            "defaultValue": "CADSession2016LegacyAPI",
            "type": "String"
        },
           "networkInterfaces_cadlegacy925_name": {
            "defaultValue": "cadsession2016legacy925",
            "type": "String"
        },
            "networkInterfaces_ukcadcontactsmasterv798_name": {
            "defaultValue": "ukcadcontactsmasterv798",
            "type": "String"
        },
        "networkSecurityGroups_CADVM_nsg_name": {
            "defaultValue": "CADSession2016VM-nsg",
            "type": "String"
        },
        "networkSecurityGroups_UKCADContactsMasterVM_nsg_name": {
            "defaultValue": "UKCADContactsMasterVM-nsg",
            "type": "String"
        },
        "publicIPAddresses_CADLegacyAPI_ip_name": {
            "defaultValue": "CADSession2016LegacyAPI-ip",
            "type": "String"
        },
        "publicIPAddresses_CADVM_ip_name": {
            "defaultValue": "CADSession2016VM-ip",
            "type": "String"
        },
        "publicIPAddresses_UKCADContactsMasterVM_ip_name": {
            "defaultValue": "UKCADContactsMasterVM-ip",
