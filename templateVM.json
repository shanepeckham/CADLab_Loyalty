            "name": "[parameters('publicIPAddresses_CADLegacyAPI_ip_name')]",
            "apiVersion": "2016-03-30",
            "location": "northeurope",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "idleTimeoutInMinutes": 4
