            "type": "String"
        },
        "virtualNetworks_CADVNET_name": {
            "defaultValue": "CADSession2016VNET",
            "type": "String"
        },
         "storageAccounts_caddiag161_name": {
            "defaultValue": "cadsession2016diag161",
            "type": "String"
        },
        "storageAccounts_disks890_name": {
            "defaultValue": "cadsession2016disks890",
            "type": "String"
        }
